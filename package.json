{
  "name": "cartonne",
  "version": "2.0.2",
  "description": "Reading and writing Content Addressable aRchive",
  "scripts": {
    "build": "rm -rf ./dist && ./node_modules/.bin/tsc -p ./tsconfig.build.json",
    "test": "./node_modules/.bin/tsm ./node_modules/uvu/bin.js . .+\\.test\\.ts",
    "test:watch": "./node_modules/.bin/watchlist src -- pnpm run test",
    "prepublishOnly": "pnpm run build"
  },
  "type": "module",
  "keywords": [
    "car",
    "ipld",
    "ipfs",
    "multiformats"
  ],
  "author": "Sergey Ukustov <sergey@ukstv.me>",
  "license": "(Apache-2.0 AND MIT)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceramicnetwork/cartonne.git"
  },
  "files": [
    "dist/"
  ],
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@ipld/dag-json": "^10.0.1",
    "@stablelib/random": "^1.0.2",
    "@types/node": "^18.16.7",
    "it-all": "^3.0.2",
    "prettier": "^2.8.8",
    "tsm": "^2.3.0",
    "typescript": "^5.0.4",
    "uvu": "^0.5.6",
    "watchlist": "^0.3.1"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^9.0.0",
    "multiformats": "^11.0.2",
    "multihashes-sync": "^1.1.0",
    "varintes": "^2.0.4"
  }
}
